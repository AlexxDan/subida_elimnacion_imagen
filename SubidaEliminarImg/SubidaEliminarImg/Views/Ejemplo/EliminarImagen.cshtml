@model List<String>
@{
    ViewData["Title"] = "EliminarImagen";
}

<h1>Eliminar Imagen</h1>

<form method="post">
    <select name="filename">
        @foreach (String filename in Model)
        {
        <option value="@filename">@filename</option>
        }
    </select>
    <button type="submit" >Eliminar</button>
</form>
<div>
    @ViewData["Info"]
</div>